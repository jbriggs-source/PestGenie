                "valueKey": "demoPicker",
                "label": "Choose Option",
                "options": [
                  {
                    "id": "opt1",
                    "text": "Option 1",
                    "value": "option1"
                  },
                  {
                    "id": "opt2",
                    "text": "Option 2",
